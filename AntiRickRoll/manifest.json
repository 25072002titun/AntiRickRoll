{
    "content_scripts": [
        {
            "js": ["background.js"],
            "matches": ["https://*.youtube.com/*"],
            "run_at": "document_start"
        }
    ],
    "name": "AntiRickRoll",
    "version": "1.1",
    "description": "Never gonna rickroll you, never gonna let you get rickrolled.",
    "manifest_version": 2,
    "web_accessible_resources": [
        "warn/warn.html"
    ],
    "permissions": [
        "storage"
    ],
    "icons": {
        "16": "icons/icon16.png",
        "48": "icons/icon48.png",
        "128": "icons/icon128.png"
    }
}